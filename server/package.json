{
  "name": "@config-vault/server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "npx dotenvx run --env-file=.env.production -- node dist/server.js",
    "dev": "npx dotenvx run --env-file=.env.development -- tsx watch src/server.ts",
    "dev:ts-node": "npx dotenvx run --env-file=.env.development -- nodemon --exec \"node --no-deprecation --import ./register.mjs\" src/server.ts",
    "migrate": "npx dotenvx run --env-file=.env.development -- tsx src/scripts/migrate.ts",
    "migrate:prod": "npx dotenvx run --env-file=.env.production -- tsx src/scripts/migrate.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@config-vault/shared": "*",
    "@dotenvx/dotenvx": "^1.49.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "http-status": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10"
  }
}
